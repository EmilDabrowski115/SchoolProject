<UserControl x:Class="WPF_LoginForm.AdminView.UsunMuzykePage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WPF_LoginForm.AdminView"
             mc:Ignorable="d" 
             d:DesignHeight="460" d:DesignWidth="600">
    <Grid Background="White">
        <StackPanel HorizontalAlignment="Center" Margin="0 75 0 0">
            <TextBlock Text="Usuń Muzykę:" FontSize="40"></TextBlock>
            <TextBlock Text="Wpisz Nazwę Utowru:" FontSize="20"></TextBlock>
            <TextBox x:Name="Utwor"></TextBox>

            <Button Content="Usuń" Click="UsunUtwor" FontWeight="Bold" Background="#F44336" Foreground="White"></Button>



        </StackPanel>


    </Grid>
</UserControl>
